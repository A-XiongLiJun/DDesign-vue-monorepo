{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  // eslint-disable-next-line prettier/prettier
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  // eslint-disable-next-line prettier/prettier
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      // eslint-disable-next-line prettier/prettier
      "@/*": [
        "./src/*"
      ]
    }
  }
}
